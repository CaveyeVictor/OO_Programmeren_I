package oefening8.domein.domein;

public class Speler {
	private String naam;
	private int vaknr;
	private static int GROOTSTENR = 63;
	
	public Speler(String naam) {
		setNaam(naam);
	}
	
	public String geefNieuwePositie(int worp) {
		return "";
	}
	
	public boolean gewonnen() {
		
	}

	public String getNaam() {
		return naam;
	}

	private void setNaam(String naam) {
		this.naam = naam;
	}

	public int getVaknr() {
		return vaknr;
	}

	public final void setVaknr(int vaknr) {
		this.vaknr = vaknr;
	}
	

}
