package ui;

import java.util.Scanner;

import domein.Speler;

public class SpelerApplicatie {

	public static void main(String[] args) {
		
		Scanner inputScanner = new Scanner(System.in);
		
		System.out.println("Naam speler 1: ");
		String naam1 = inputScanner.nextLine();
		System.out.println("Naam speler 2: ");
		String naam2 = inputScanner.nextLine();
		
		Speler speler1 = new Speler(naam1);
		Speler speler2 = new Speler(naam2);
		
		
		
		Speler aSpeler = new Speler("Jan");
		System.out.println(aSpeler.geefNieuwePositie(5*11));
		System.out.println(aSpeler.geefNieuwePositie(6));
		System.out.println(aSpeler.geefNieuwePositie(6));

		
		

	}

}
