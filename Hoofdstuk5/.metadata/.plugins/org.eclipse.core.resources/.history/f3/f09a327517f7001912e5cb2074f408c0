package ui;

import oefening8.domein.domein.Speler;

public class SpelerApplicatie {

	public static void main(String[] args) {
		Speler aSpeler = new Speler("Jan");
		System.out.println(aSpeler.geefNieuwePositie(6));
		System.out.println(aSpeler.geefNieuwePositie(20));
		System.out.println(aSpeler.geefNieuwePositie(6));
		System.out.println(aSpeler.geefNieuwePositie(4));
		

	}

}
