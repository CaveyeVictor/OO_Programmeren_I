
public class Telraam
{

	public static void main(String[] args)
	{
		int getal = 2;
		int getalpluseen = getal + 1, getalmineen = getal -1;
		System.out.println("Getal = " + getal);
		for (int i=0; i < 2; i++){
			System.out.println("Getal + 1 = " + getalpluseen);
			System.out.println("Getal - 1 = " + getalmineen;
		}
	}

}
